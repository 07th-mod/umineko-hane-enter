	getspsize %meta_tmp4,%meta_tmp5,%meta_tmp6,%meta_tmp7
	;画像の幅/4　で画像中心からの幅が求められるので（立ち位置のX座標からその幅を引く）
	div %meta_tmp5,2
	if %meta_tmp1 = l mov %x,160
	if %meta_tmp1 = c mov %x,320
	if %meta_tmp1 = r mov %x,480

	sub %x,%meta_tmp5
	amsp %meta_tmp4,%x,0
	print %meta_tmp3
return

